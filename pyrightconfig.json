{
  "include": ["src"],
  "exclude": ["**/node_modules", "**/__pycache__", ".venv"],
  "pythonVersion": "3.12",
  "pythonPlatform": "Windows",
  "typeCheckingMode": "strict",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportUnusedVariable": true,
  "venvPath": ".",
  "venv": ".venv",
    "useLibraryCodeForTypes": true
}
